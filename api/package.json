{
  "name": "auth",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "babel-node ./src/app.js",
    "build": "rm -rf build && babel src -d build --copy-files --source-maps inline && npm run build-copy-files",
    "build-copy-files": "cp app.js ./build && cp server.js ./build && cp build.process.js ./build/process.config.js",
    "production": "pm2-runtime ./build.process.js",
    "development": "pm2-runtime ./build/process.config.js",
    "pm2-ls": "pm2 ls",
    "pm2-status": "pm2 status",
    "pm2-logs": "pm2 logs 0 --lines 1000",
    "pm2-monit": "pm2 monit",
    "pm2-stop": "pm2 stop pm2",
    "pm2-del": "pm2 delete all",
    "setup": "yarn && knex migrate:rollback --all --env development && knex migrate:latest --env development && knex seed:run",
    "migrate": "knex migrate:latest --env development",
    "migrate-build": "knex migrate:latest --knexfile ./build/knexfile.js --env development",
    "migrate-rollback": "knex migrate:rollback --all --env development",
    "migrate-up_dev": "knex migrate:up --env development",
    "migrate-down_dev": "knex migrate:down --env development",
    "create-migration": "knex migrate:make migration_name",
    "create-seed": "knex seed:make seed_name",
    "seed": "knex seed:run",
    "migrate-complete": "npm run migrate-rollback && npm run migrate && npm run seed"
  },
  "author": "Avetius",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.11.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^5.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "express": "^4.17.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.4.18",
    "objection": "^3.0.1",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pm2": "^5.2.2",
    "slack-webhook": "^1.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.20.2",
    "@babel/node": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0"
  }
}
